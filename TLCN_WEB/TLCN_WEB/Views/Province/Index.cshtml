@model IEnumerable<TLCN_WEB.Models.Province>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ProvinceID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ProvinceName)
        </th>       
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ProvinceID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProvinceName)
            </td>
           

            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.ProvinceID }) |
                @Html.ActionLink("Detail", "Detail", new { id = item.ProvinceID }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.ProvinceID })
            </td>
        </tr>
    }

</table>

