@model IEnumerable<TLCN_WEB.Models.Menu>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.MenuID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Store_ID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MenuName)
        </th>          
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.MenuID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Store_ID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MenuName)
            </td>



            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.MenuID }) |
                @Html.ActionLink("Detail", "Detail", new { id = item.MenuID }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.MenuID })
            </td>
        </tr>
    }

</table>

